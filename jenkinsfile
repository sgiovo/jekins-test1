pipeline {
  agent any

  stages {
    stage('Create File') {
      steps {
        echo 'Creating file..'
        def scriptGroovy = load 'fileCreate.groovy'
        scriptGroovy.createHello('World')
      }
    }
    stage('Print Hello') {
      steps {
        script {
          def scriptGroovy = load 'print.groovy'
          scriptGroovy.print()
        }
      }
    }
  }
}